<script type="text/javascript">
	function showSubnav(linkObj){
		$('#mobile-in-page-nav ul.nav').toggle();
		if ($(linkObj).hasClass('active')) {
			$(linkObj).removeClass('active');
		} else {
			$(linkObj).addClass('active');
		}
	}
</script>
					
