                <script type="text/javascript">
                    var sticker = 0;
                    $(document).ready(function() {
                         $('a.prettyPhoto').prettyPhoto({
                              show_title: false,
                              overlay_gallery: false,
                              social_tools: ''
                         });
                         $(".photoGallery").click(function() {
                              $("a[rel^='prettyPhoto']:first").click();
                         });
                         if (Modernizr.touch) {
                              $('ul.nav').on('touchstart click', 'a.toggler', function(e){
                                   e.stopPropagation();
                                   e.preventDefault();
                                   var t = $(this);
                                   var target = t.attr('rel');
                                   var opened = t.hasClass('open-menu');
                                   $("ul.nav a.toggler").removeClass('open-menu');
                                   t.addClass('open-menu');
                                   if (opened) {
                                        $(target).fadeOut(1000, function() {
                                             t.removeClass('open-menu');
                                        });
                                   } else {
                                        t.addClass('open-menu');
                                        $(".flyout-pane").hide();
                                        $(target).fadeIn(1000);
                                   }
                                   return false;
                              });
                         } else {
                              $("ul.nav a.toggler").hoverIntent(function() {
                                   var t = $(this);
                                   var target = t.attr('rel');
                                   $("ul.nav a.toggler").removeClass('open-menu');
                                   $(".flyout-pane").hide();
                                   $(target).slideDown();
                                   t.addClass('open-menu');
                              }, function() {
                                   if (!sticker){
                                        var t = $(this);
                                        //var target = t.attr('href');
                                        t.removeClass('open-menu');
                                        //target.hide(1000);
                                        $(".flyout-pane").slideUp();
                                        $("ul.nav a.toggler").removeClass('open-menu');
                                   }
                              });

                              $(".flyout-pane").mouseenter(function() {
                                   sticker = 1;
                              });

                              $("nav.main-nav, nav.top-nav").mouseleave(function() {
                                   $(".flyout-pane").slideUp();
                                   $("ul.nav a.toggler").removeClass('open-menu');
                              });

                         }
                    });
                </script>


